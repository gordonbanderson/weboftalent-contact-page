{
    "name": "weboftalent\/contact-page",
    "description": "SilverStripe module to add a contact page with optional map to your site",
    "type": "silverstripe-module",
    "keywords": [
        "silverstripe",
        "contact",
        "website message",
        "contact page",
        "map"
    ],
    "authors": [
        {
            "name": "Gordon Anderson",
            "email": "gordon.b.anderson@gmail.com",
            "homepage": "https:\/\/github.com\/gordonbanderson\/weboftalent-contact-page",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https:\/\/github.com\/gordonbanderson\/weboftalent-contact-page\/issues"
    },
    "require": {
        "php": ">=7.1",
        "silverstripe\/recipe-cms": "4.x-dev",
        "smindel\/silverstripe-gis": "^1"
    },
    "require-dev": {
        "ergebnis\/composer-normalize": "^2.6",
        "phpunit\/phpunit": "^5.7",
        "suilven\/php-travis-enhancer": "^1.0",
        "slevomat\/coding-standard": "^6.3",
        "php-parallel-lint\/php-parallel-lint": "^1.2",
        "php-parallel-lint\/php-console-highlighter": "^0.5.0",
        "phpstan\/phpstan-strict-rules": "^0.12.4",
        "phpstan\/extension-installer": "^1.0",
        "symbiote/silverstripe-phpstan": "^4.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "BSD-3-Clause",
    "scripts": {
        "checkcs": "vendor\/bin\/phpcs --standard=ruleset.xml --extensions=php --tab-width=4 -sp src tests",
        "fixcs": "vendor\/bin\/phpcbf --standard=ruleset.xml --extensions=php --tab-width=4 -sp src tests",
        "checkCode": "composer checkcs && composer lint && composer phpstan",
        "lint": "vendor\/bin\/parallel-lint src\/ tests\/",
        "phpstan": "vendor\/bin\/phpstan analyse --level=6 -c tests\/phpstan.neon src\/"
    }
}
